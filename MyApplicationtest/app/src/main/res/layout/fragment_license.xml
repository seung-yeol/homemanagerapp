<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="15dp">

    <TextView
        android:textColor="#000"
        android:textStyle="bold"
        android:text="Thank you Image"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView3" />
    <TextView
        android:autoLink="web"
        android:layout_marginLeft="20dp"
        android:text="@string/open_image"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"/>
    <TextView
        android:textColor="#000"
        android:textStyle="bold"
        android:text="Emotions Assets (Icon)"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        android:autoLink="web"
        android:layout_marginLeft="20dp"
        android:text="@string/open_icon_face"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"/>
    <TextView
        android:textColor="#000"
        android:textStyle="bold"
        android:text="Essetial icons (Icon)"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        android:autoLink="web"
        android:layout_marginLeft="20dp"
        android:text="@string/open_icon_main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"/>
    <TextView
        android:textColor="#000"
        android:textStyle="bold"
        android:text="other icons"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        android:autoLink="web"
        android:layout_marginLeft="20dp"
        android:text="@string/open_icon_other"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"/>
    <TextView
        android:textColor="#000"
        android:textStyle="bold"
        android:text="SwipeToDismiss"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        android:layout_marginLeft="20dp"
        android:text="@string/open_swipe"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:autoLink="web"/>
</LinearLayout>